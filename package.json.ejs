{
  "private": true,
  "name": "<%= config.name %>",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx husky install",
    "dev": "doer dev",
    "build": "doer build"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --write",
      "eslint"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/runtime": "^7.16.7",
    "@babel/runtime-corejs3": "^7.16.7",
    "<%= packages['@doerjs/cli'].name %>": "<%= packages['@doerjs/cli'].version %>",
    "<%= packages['@doerjs/eslint-config'].name %>": "<%= packages['@doerjs/eslint-config'].version %>",
    "<%= packages['@doerjs/prettier-config'].name %>": "<%= packages['@doerjs/prettier-config'].version %>",
    <% if (config.style === 'less') { %>
    "<%= packages['@doerjs/plugin-less'].name %>": "<%= packages['@doerjs/plugin-less'].version %>",
    <% } %>
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5"
  }
}